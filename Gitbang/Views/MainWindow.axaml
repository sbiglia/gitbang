<gitbang:FluentWindow xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:Gitbang.ViewModels;assembly=Gitbang"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:repos="clr-namespace:Gitbang.Views.ReposExplorer"
        xmlns:views="clr-namespace:Gitbang.Views"
        xmlns:gitbang="clr-namespace:Gitbang"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Gitbang.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Gitbang">
  <DockPanel>
    <repos:ReposTreeView DockPanel.Dock="Left"></repos:ReposTreeView>
    <views:RepoDocumentView DockPanel.Dock="Right"></views:RepoDocumentView>
  </DockPanel>
  
</gitbang:FluentWindow>
